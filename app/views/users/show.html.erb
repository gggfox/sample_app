<% provide(:title, @user.name) %>
<%= Time.now %>
<div class="row">
    <aside class="col-md-4">
        <section class="user_info">
            <h1>
                <%= gravatar_for @user %>
                <%= @user.name %>
                <div>
                    <%= @user.email %>
                </div>
                <span><%= @user.created_at %></span>
            </h1>       
        </section>
    </aside>
</div>


